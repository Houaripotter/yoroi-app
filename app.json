{
  "expo": {
    "name": "Yoroi",
    "slug": "yoroi",
    "displayName": "Yoroi",
    "description": "Track your fitness journey with Yoroi",
    "githubUrl": "https://github.com/your-username/yoroi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.houari.yoroi",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Yoroi a besoin de lire vos données de poids pour suivre votre progression.",
        "NSHealthUpdateUsageDescription": "Yoroi a besoin d'écrire votre poids pour synchroniser vos données."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.houari.yoroi"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3743da81-d416-4eb3-b7a5-45e2094947cb"
      }
    }
  }
}